<LinearLayout
    xmlns:android = "http://schemas.android.com/apk/res/android"
    xmlns:tools = "http://schemas.android.com/tools"
    android:layout_width = "match_parent"
    android:layout_height = "match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context = "com.perfect.prism.Activity.LocaReportValueActivity">


    <TextView
        android:id="@+id/txtDate"
        android:paddingLeft="15dp"
        android:textStyle="italic|bold"
        android:textColor="@color/gray"
        android:layout_marginTop="10dp"
        android:fontFamily="sans-serif-condensed"
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"/>



    <LinearLayout
        android:id="@+id/lnrReport"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"

        android:layout_margin="10dp"
        android:visibility="visible"
        android:background="@drawable/button_corner1"
        android:layout_marginBottom="80dp"
        android:padding="5dp">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_weight="2"
                android:id="@+id/lnr_agent_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal">
            <TextView
                android:id="@+id/txtAgent_Client_Header"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:fontFamily="sans-serif-condensed"
                android:layout_gravity="center_vertical"
                android:text="Agent Name"
                android:textAlignment="gravity"
                android:textColor="@color/white"
                android:textStyle="bold" />

            </LinearLayout>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="?android:attr/dividerVertical"/>

            <LinearLayout
                android:layout_weight="1.95"
                android:id="@+id/lnr_client_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:layout_gravity="center_vertical"
                android:fontFamily="sans-serif-condensed-light"
                android:text="Client Name"
                android:textAlignment="gravity"
                android:layout_marginRight="5dp"
                android:textColor="@color/white"
                android:textStyle="bold" />
            </LinearLayout>

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="?android:attr/dividerVertical"/>
            <LinearLayout
                android:layout_weight="1.9"
                android:id="@+id/ticketnumber"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal"
                android:visibility="visible"
                >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="gravity"
                    android:gravity="center_horizontal"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="sans-serif-condensed-light"
                    android:text="Tkt No"
                    android:layout_marginRight="5dp"
                    android:textColor="@color/white"
                    android:textStyle="bold" />
            </LinearLayout>

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="?android:attr/dividerVertical"/>

            <LinearLayout
                android:layout_weight="1.80"
                android:id="@+id/lnr_location"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal"
                >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="gravity"
                    android:gravity="center_horizontal"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="sans-serif-condensed-light"
                    android:text="St Location"
                    android:layout_marginRight="5dp"
                    android:textColor="@color/white"
                    android:textStyle="bold" />

            </LinearLayout>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="?android:attr/dividerVertical"/>

            <LinearLayout
                android:layout_weight="2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal"
                >

                <TextView
                    android:id="@+id/txt_stime"
                    android:textAlignment="center"
                    android:layout_gravity="center"
                    android:layout_width="match_parent"
                    android:textColor="@color/white"
                    android:layout_height="wrap_content"
                    android:text="St Time"

                    />
            </LinearLayout>

            <View
                android:background="?android:attr/dividerVertical"
                android:layout_width="1dp"
                android:layout_height="match_parent"/>
            <LinearLayout
                android:layout_weight="1.80"
                android:id="@+id/lnr_end_location"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal"
                >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="gravity"
                    android:gravity="center_horizontal"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="sans-serif-condensed-light"
                    android:text="En Location"
                    android:layout_marginRight="5dp"
                    android:textColor="@color/white"
                    android:textStyle="bold" />

            </LinearLayout>
            <View
                android:id="@+id/vel"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="?android:attr/dividerVertical"/>

            <LinearLayout
                android:layout_weight="2"
                android:id="@+id/lnr_end_time"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal"
                >

                <TextView
                    android:id="@+id/txt_etime"
                    android:textAlignment="center"
                    android:layout_gravity="center"
                    android:layout_width="match_parent"
                    android:textColor="@color/white"
                    android:layout_height="wrap_content"
                    android:text="En Time"

                    />
            </LinearLayout>

            <View
                android:id="@+id/vet"
                android:background="?android:attr/dividerVertical"
                android:layout_width="1dp"
                android:layout_height="match_parent"/>




            <LinearLayout
                android:layout_weight="1.9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal"
                android:visibility="gone"
                >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="gravity"
                    android:gravity="center_horizontal"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="sans-serif-condensed-light"
                    android:text="Start Time"
                    android:layout_marginRight="5dp"
                    android:textColor="@color/white"
                    android:textStyle="bold" />

            </LinearLayout>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="?android:attr/dividerVertical"
                android:visibility="gone"
                android:id="@+id/a"/>

            <LinearLayout
                android:layout_weight="2"
                android:id="@+id/lnr_assign_ticket"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal"
                android:visibility="gone"
                >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAlignment="gravity"
                android:gravity="center_horizontal"
                android:layout_gravity="center_vertical"
                android:fontFamily="sans-serif-condensed-light"
                android:text="Assign tkt"
                android:layout_marginRight="5dp"
                android:textColor="@color/white"
                android:textStyle="bold" />
            </LinearLayout>

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="?android:attr/dividerVertical"
                android:visibility="gone"
                android:id="@+id/b"/>
            <LinearLayout
                android:layout_weight="2"
                android:id="@+id/lnr_closed_ticket"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal"
                android:visibility="gone"
                >


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAlignment="gravity"
                android:gravity="center_horizontal"
                android:layout_gravity="center_vertical"
                android:fontFamily="sans-serif-condensed-light"
                android:text="Clsd tkt"
                android:layout_marginRight="5dp"
                android:textColor="@color/white"
                android:textStyle="bold" />
            </LinearLayout>

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="?android:attr/dividerVertical"
                android:visibility="gone"
                android:id="@+id/c"/>

            <LinearLayout
                android:layout_weight="2"
                android:id="@+id/lnr_software_pending"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal"
                android:visibility="gone">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="sans-serif-condensed-light"
                android:gravity="center_horizontal"
                android:layout_gravity="center_vertical"
                android:text="Sftw Pndg"
                android:textAlignment="gravity"
                android:textColor="@color/white"
                android:textStyle="bold" />
            </LinearLayout>

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="?android:attr/dividerVertical"
                android:visibility="gone"
                android:id="@+id/d"/>
            <LinearLayout
                android:layout_weight="2"
                android:id="@+id/lnr_balance"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal"
                android:visibility="gone">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="sans-serif-condensed-light"
                android:gravity="center_horizontal"
                android:layout_gravity="center_vertical"
                android:text="Balance"
                android:textAlignment="gravity"
                android:textColor="@color/white"
                android:textStyle="bold" />
            </LinearLayout>



            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="?android:attr/dividerVertical"
                android:visibility="gone"
                android:id="@+id/e"/>
            <LinearLayout
                android:layout_weight="1.95"
                android:id="@+id/ll_status"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:orientation="horizontal"
                android:visibility="gone"
                >


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="gravity"
                    android:gravity="center_horizontal"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="sans-serif-condensed-light"
                    android:text="status"
                    android:layout_marginRight="5dp"
                    android:textColor="@color/white"
                    android:textStyle="bold" />
            </LinearLayout>

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="?android:attr/dividerVertical"
                android:visibility="gone"
                android:id="@+id/f"/>

            <LinearLayout
                android:layout_weight="2"
                android:id="@+id/ll_asssigntime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:visibility="gone"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-condensed-light"
                    android:gravity="center_horizontal"
                    android:layout_gravity="center_vertical"
                    android:text="Ass tim"
                    android:textAlignment="gravity"
                    android:textColor="@color/white"
                    android:textStyle="bold" />
            </LinearLayout>

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="?android:attr/dividerVertical"
                android:visibility="gone"
                android:id="@+id/g"/>
            <LinearLayout
                android:layout_weight="2"
                android:id="@+id/ll_actual_time"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:visibility="gone"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif-condensed-light"
                    android:gravity="center_horizontal"
                    android:layout_gravity="center_vertical"
                    android:text="act tim"
                    android:textAlignment="gravity"
                    android:textColor="@color/white"
                    android:textStyle="bold" />
            </LinearLayout>

        </LinearLayout>


        <android.support.v7.widget.RecyclerView
            android:background="@color/white"
            android:id="@+id/rcyLocation"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />



    </LinearLayout>

</LinearLayout>
